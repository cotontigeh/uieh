{
  "name": "uieh",
  "version": "0.0.1-alpha",
  "description": "Ui library for react",
  "author": "Adrien E.",
  "license": "MIT",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/uieh.umd.cjs",
  "module": "./dist/uieh.js",
  "exports": {
    ".": {
      "import": "./dist/uieh.js",
      "require": "./dist/uieh.umd.cjs"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./theme/colors.theme.css": "./dist/theme/colors.theme.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cotontigeh/uieh.git"
  },
  "scripts": {
    "dev": "vite -c vite.docs.config.ts",
    "build": "tsc -b -d && vite build && cp -r src/theme dist/theme",
    "build-docs": "tsc -b && vite build -c vite.docs.config.ts",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@radix-ui/react-slot": "^1.1.1",
    "clsx": "^2.1.1",
    "cva": "^1.0.0-beta.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-router": "^7.2.0",
    "react-scan": "^0.3.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^22.15.29",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@headlessui/react": "^2.0.0",
    "react": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "@headlessui/react": {
      "optional": false
    }
  },
  "resolutions": {
    "esbuild": "^0.25.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
